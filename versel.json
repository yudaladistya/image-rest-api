{
    "redirects": [
        {
            "source": "/",
            "destination": "https://github.com/dannsdb/random-image-rest-api"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, s-maxage=60, stale-while-revalidate"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}
